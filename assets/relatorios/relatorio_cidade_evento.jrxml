	</parameter>
	<queryString>
		<![CDATA[SELECT gab_pk_seq_evc,
       gab_pk_seq_eve,
       gab_desc_eve,
       gab_desc_evc,
       CASE gab_dia_evc
        WHEN 1 THEN 'Domingo'
        WHEN 2 THEN 'Segunda'
        WHEN 3 THEN 'Terça'
        WHEN 4 THEN 'Quarta'
        WHEN 5 THEN 'Quinta'
        WHEN 6 THEN 'Sexta'
        WHEN 7 THEN 'Sábado'
        else ''
        END AS Dia_Semana,
        CASE gab_datacomemorativa_evc
        WHEN 0000-00-00 THEN ''
        else gab_datacomemorativa_evc END AS DATA_COMEMORATIVA
FROM gabtbevc
LEFT OUTER JOIN gabtbeve on gab_pk_seq_eve = gab_seqeve_evc
WHERE gab_seqcid_evc = $P{gab_seqcid_evc}]]>
