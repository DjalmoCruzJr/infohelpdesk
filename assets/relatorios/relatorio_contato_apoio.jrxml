	<queryString>
		<![CDATA[select gab_pk_seq_pol,
       apoio.gab_nome_con,
       gab_desc_cgo,
       case gab_tipo_cgo when 0 then "Publico"
       else "Privado"
      end as tipo_cargo
from gabtbpol
left join gabtbcon apoioado ON apoioado.gab_pk_seq_con = gab_seqconpai_pol
left join gabtbcon apoio    ON apoio.gab_pk_seq_con    = gab_seqcon_pol
left join gabtbcgo          ON apoio.gab_seqcgo_con    = gab_pk_seq_cgo
where apoioado.gab_pk_seq_con = $P{gab_seqconpai_pol}]]>
